{
  "name": "zf3buch/zendframework-center",
  "description": "Zend Framework Center Example Application for the Zend Framework 3",
  "license": "The MIT License (MIT)",
  "type": "project",
  "authors": [
    {
      "name": "Ralf Eggert",
      "email": "ralf@travello.de",
      "homepage": "http://www.travello.de",
      "role": "Developer"
    }
  ],
  "homepage": "http://www.zendframeworkbuch.de/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
    "zendframework/zend-console": "^2.6",
    "zendframework/zend-mvc": "^3.0",
    "twitter/bootstrap": "~3.3",
    "fortawesome/font-awesome": "~4.4",
    "frameworks/jquery": "~1.11",
    "zendframework/zend-navigation": "^2.7",
    "zendframework/zend-paginator": "^2.7",
    "zendframework/zend-hydrator": "^2.2",
    "zendframework/zend-filter": "^2.7",
    "zendframework/zend-db": "^2.8",
    "zendframework/zend-inputfilter": "^2.7",
    "zendframework/zend-form": "^2.8",
    "zendframework/zend-math": "^3.0",
    "zendframework/zend-session": "^2.7",
    "zendframework/zend-i18n": "^2.7",
    "ralfeggert/travello-view-helper": "^1.3",
    "ralfeggert/travello-filter": "^1.2",
    "ckeditor/ckeditor": "^4.5",
    "zendframework/zend-mvc-i18n": "^1.0",
    "zendframework/zend-permissions-acl": "^2.6",
    "zendframework/zend-authentication": "^2.5",
    "zendframework/zend-mvc-plugin-identity": "^1.0",
    "doctrine/doctrine-module": "dev-master",
    "doctrine/doctrine-orm-module": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "zendframework/zend-developer-tools": "^1.1",
    "san/san-session-toolbar": "^0.6.3",
    "zendframework/zend-test": "^3.0",
    "phpunit/dbunit": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "module/Application/src/",
      "AdvertFrontend\\": "module/AdvertFrontend/src/",
      "AdvertBackend\\": "module/AdvertBackend/src/",
      "AdvertModel\\": "module/AdvertModel/src/",
      "AdvertRest\\": "module/AdvertRest/src/",
      "CompanyModel\\": "module/CompanyModel/src/",
      "CompanyBackend\\": "module/CompanyBackend/src/",
      "UserModel\\": "module/UserModel/src/",
      "UserFrontend\\": "module/UserFrontend/src/",
      "UserBackend\\": "module/UserBackend/src/",
      "Scripts\\": "script/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApplicationTest\\": "module/Application/test/",
      "AdvertFrontendTest\\": "module/AdvertFrontend/test/",
      "AdvertBackendTest\\": "module/AdvertBackend/test/",
      "AdvertModelTest\\": "module/AdvertModel/test/",
      "CompanyModelTest\\": "module/CompanyModel/test/",
      "CompanyBackendTest\\": "module/CompanyBackend/test/",
      "UserModelTest\\": "module/UserModel/test/",
      "UserFrontendTest\\": "module/UserFrontend/test/",
      "UserBackendTest\\": "module/UserBackend/test/"
    }
  },
  "extra": [],
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
    "post-update-cmd": "Scripts\\Composer::postUpdate"
  }
}
